

SUB(CheckLikesPersonal)    // Функция делает анализ экрана персональной странциы ВК, если находит место для лайка - ставит его   
   WAITMS(RND(1001, 1200))
   GETSCREEN()
   IF_PIXEL_IN (883, 247, 940, 1026, 12959160)
      LCLICK($_return1, $_return2)
   END_IF
END_SUB


