   LCLICK(45,172)
   WAITMS(RND(3001, 3200))
   LCLICK(546,302)
   WAITMS(RND(3001, 3200))
   LCLICK(821,301)
   WAITMS(RND(3001, 3200))		// заходим в группу
   $var = 0
   WHILE($var < 8)
      INC($var,1)  
      WHEELDOWN
      WAITMS(500)
      WHEELDOWN
      WAITMS(500)
      WHEELDOWN
      WAITMS(500)
      WHEELDOWN
      WAITMS(500)
      WHEELDOWN
